<% include ../../partials/adminHead %>
<br>
<H1>Edit Broker: <%=brokers.Broker_Symbol%> </H1>
<hr>
<div class="container">
    <br>
    <br>
    <form class="register-box" action="/admin/broker/update/<%=brokers.Broker_Symbol%>?_method=PUT" method="POST">

        <div class="form-group row">
            <label class="col-sm-4 col-form label">Broker_Symbol</label>
            <div class="col-sm-6">
                <input type="text" class="form-control" value="<%=brokers.Broker_Symbol%>" name="Broker_Symbol">
            </div>
        </div>
        <div class="form-group row">
                <label class="col-sm-4 col-form label">Broker_Name</label>
                <div class="col-sm-6">
                    <input type="text" class="form-control" value="<%=brokers.Broker_Name%>" name="Broker_Name">
                </div>
            </div>
        <div class="form-group row">
            <label class="col-sm-4 col-form label">Broker_Address</label>
            <div class="col-sm-6">
                <input type="text" class="form-control" value="<%=brokers.Broker_Address%>" name="Broker_Address">
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-4 col-form label">Broker_Email</label>
            <div class="col-sm-6">
                <input type="text" class="form-control" value="<%=brokers.Broker_Email%>" name="Broker_Email">
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-4 col-form label">Broker_Phone</label>
            <div class="col-sm-6">
                <input type="text" class="form-control" value="<%=brokers.Broker_Phone%>" name="Broker_Phone">
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-4 col-form label">Broker_Commission</label>
            <div class="col-sm-6">
                <input type="number" class="form-control" value="<%=brokers.Broker_Commission%>" name="Broker_Commission">
            </div>
        </div>
        <button class="btn btn-primary btn-block" type="submit">Edit</button>
    </form>
    <div>



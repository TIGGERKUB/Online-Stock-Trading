<% include ../../../partials/adminHead %>

<br>
<table class="table table-dark">
  <thead>
    <tr>
      <td scope="col"></td>
      <th scope="col">Broker_Symbol</th>
        <th scope="col">Broker_Name</th>
        <th scope="col">Broker_Address</th>
        <th scope="col">Broker_Email</th>
        <th scope="col">Broker_Phone</th>
        <th scope="col">Broker_Commission</th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
      <% brokers.forEach(function(broker){ %>
    <tr>
      <td><input type="checkbox" name="checkAll" class="checkSingle"></td>
      <th scope="row"><%=broker.Broker_Symbol%></th>
        <td><%=broker.Broker_Name%></td>
        <td><%=broker.Broker_Address%></td>
        <td><%=broker.Broker_Email%></td>
        <td><%=broker.Broker_Phone%></td>
        <td><%=broker.Broker_Commission%></td>
      <td>
        <form action="/admin/broker/<%=broker.Broker_Symbol%>/edit" method="GET">
          <button class="btn btn-warning" aria-hidden="true"><i class="fa fa-wrench" aria-hidden="true"></i></button>
        </form>
      </td>
      <td>
        <form action="/admin/broker/<%=broker.Broker_Symbol%>?_method=DELETE" method="POST">
          <button class="btn btn-danger" aria-hidden="true"><i class="fa fa-trash" aria-hidden="true"></i></button>
        </form>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col"><input type="checkbox" name="checkedAll" id="checkedAll"> Check all</th>
      <th class="text-right pr-4">
        <form action="/" method="POST">
          <button class="btn btn-danger" aria-hidden="true" style="margin-right: 5px"><i class="fa fa-trash" aria-hidden="true"></i></button>
        </form>
      </th>
    </tr>
  </thead>
</table>



<% include ../../../partials/adminFoot %>
  
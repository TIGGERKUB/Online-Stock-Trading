<% include ../../../partials/adminHead %>

<br>
<table class="table table-dark">
  <thead>
    <tr>
      <td scope="col"></td>
      <th scope="col">Stock_Symbol</th>
      <th scope="col">Stock_Volume</th>
      <th scope="col">Market_Type</th>
      <th scope="col">Company_Name</th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <% stocks.forEach(function(stock){ %>
    <tr>
      <td><input type="checkbox" name="checkAll" class="checkSingle"></td>
      <th scope="row"><%=stock.Stock_Symbol%></th>
      <td><%=stock.Stock_Volume%></td>
      <td><%=stock.Market_Type%></td>
      <td><%=stock.Company_Name%></td>
      <td>
        <form action="/admin/stock/<%=stock.Stock_Symbol%>/edit" method="GET">
          <button class="btn btn-warning" aria-hidden="true"><i class="fa fa-wrench" aria-hidden="true"></i></button>
        </form>
      </td>
      <td>
        <form action="/admin/stock/<%=stock.Stock_Symbol%>?_method=DELETE" method="POST">
          <button class="btn btn-danger" aria-hidden="true"><i class="fa fa-trash" aria-hidden="true"></i></button>
        </form>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col"><input type="checkbox" name="checkedAll" id="checkedAll"> Check all</th>
      <th class="text-right pr-4">
        <form action="/" method="POST">
          <button class="btn btn-danger" aria-hidden="true" style="margin-right: 5px"><i class="fa fa-trash" aria-hidden="true"></i></button>
        </form>
      </th>
    </tr>
  </thead>
</table>



<% include ../../../partials/adminFoot %>
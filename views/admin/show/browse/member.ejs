<% include ../../../partials/adminHead %>

<br>
<table class="table table-dark">
  <thead>
    <tr>
      <td scope="col"></td>
      <th scope="col">Trader_Personal_ID</th>
        <th scope="col">Username</th>
        <th scope="col">Password</th>
        <th scope="col">Trader_Fname</th>
        <th scope="col">Trader_Lname</th>
        <th scope="col">Trader_Age</th>
        <th scope="col">Trader_Address</th>
        <th scope="col">Trader_Email</th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
      <% members.forEach(function(member){ %>
    <tr>
      <td><input type="checkbox" name="checkAll" class="checkSingle"></td>
      <th scope="row"><%=member.Trader_Personal_ID%></th>
        <td><%=member.Username%></td>
        <td><%=member.Password%></td>
        <td><%=member.Trader_Fname%></td>
        <td><%=member.Trader_Lname%></td>
        <td><%=member.Trader_Age%></td>
        <td><%=member.Trader_Address%></td>
        <td><%=member.Trader_Email%></td>
      <td>
        <form action="/admin/member/<%=member.Trader_Personal_ID%>/edit" method="GET">
          <button class="btn btn-warning" aria-hidden="true"><i class="fa fa-wrench" aria-hidden="true"></i></button>
        </form>
      </td>
      <td>
        <form action="/admin/member/<%=member.Trader_Personal_ID%>?_method=DELETE" method="POST">
          <button class="btn btn-danger" aria-hidden="true"><i class="fa fa-trash" aria-hidden="true"></i></button>
        </form>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col"><input type="checkbox" name="checkedAll" id="checkedAll"> Check all</th>
      <th class="text-right pr-4">
        <form action="/" method="POST">
          <button class="btn btn-danger" aria-hidden="true" style="margin-right: 5px"><i class="fa fa-trash" aria-hidden="true"></i></button>
        </form>
      </th>
    </tr>
  </thead>
</table>



